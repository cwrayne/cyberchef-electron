{
  "name": "cyberchef-electron",
  "version": "1.0.0",
  "description": "CyberChef, but desktop this time",
  "keywords": [
    "CyberChef"
  ],
  "license": "ISC",
  "author": "Dashiell Benton",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist": "electron-builder --publish never"
  },
  "build": {
    "appId": "com.dbenton.cyberchef-electron",
    "productName": "CyberChef Electron",
    "copyright": "Copyright Â© 2025 Dashiell Benton",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  },
  "devDependencies": {
    "electron": "^29.0.0",
    "electron-builder": "^24.9.0"
  },
  "dependencies": {}
}
